
#include "uart.h"

#define GPIOAEN				(1U << 0);
#define GPIOA_5				(1U << 5);

#define LED_PIN				GPIOA_5


char key;
int main(void)
{
	char message[31] = "Hello From Stm32 DMA Transfer\n\r";

	// Clock Access to GPIOA
	RCC->AHB1ENR |= GPIOAEN;

	// PA5 AS OUTPUT
	GPIOA->MODER |= (1U << 10);
	GPIOA->MODER &= (~(1U << 11));

	UART2TX_Init();
	DMA1_Stream6_Init((uint32_t) message, (uint32_t) USART2->DR,31 );

	while(1)
	{


	}
}





